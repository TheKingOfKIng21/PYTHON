    "\n",
    "def factorial(n):\n",
    "   \n",
    "   if  n == 0:\n",
    "       return 1\n",
    "   else:\n",
    "       return n * factorial(n-1)\n",
    "   \n",
    "print(factorial(2))\n",
    "print(factorial(7))"
   ]
  }
