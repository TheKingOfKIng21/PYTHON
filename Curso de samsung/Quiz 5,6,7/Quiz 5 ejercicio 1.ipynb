    "def bubblesort(S):\n",
    "    n = len(S)\n",
    "    for i in range(n):\n",
    "        print(S)\n",
    "        for j in range(n-1):\n",
    "            if S[j]>S[j +1]:\n",
    "                S[j], S[j+1] = S[j+1], S[j]"
