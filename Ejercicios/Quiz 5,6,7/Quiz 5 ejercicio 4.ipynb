   "source": [
    "S = [15,10,12,20,25,13,22]\n",
    "\n",
    "def partition1(lst, size):\n",
    "    for i in range(0, len(lst), size):\n",
    "        yield lst[i : i+size]\n",
    "        \n",
    "n = 2\n",
    "\n",
    "\n",
    "p_list = list(partition(S, n))\n",
    "\n",
    "\n",
    "print(\"Lista original: \")\n",
    "print(S)\n",
    "\n",
    "print(\"Lista particionado:\")\n",
    "print(p_list)"
   ]
  }
